import system'routines;
import extensions;

public program()
{
    var Doors := Array.allocate(100).populate:(n=>false);
    for(int i := 0, i < 100, i := i + 1)
    {
        for(int j := i, j < 100, j := j + i + 1)
        {
            Doors[j] := Doors[j].Inverted
        }
    };

    for(int i := 0, i < 100, i := i + 1)
    {
        console.printLine("Door #",i + 1," :",Doors[i].iif("Open","Closed"))
    };

    console.readChar()
}
