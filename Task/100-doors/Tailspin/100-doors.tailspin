source hundredDoors
  @: [ 1..100 -> 0 ];
  templates toggle
    def jump: $;
    $jump..100:$jump -> \(when <?($@hundredDoors($) <=0>)> do @hundredDoors($): 1;  otherwise @hundredDoors($): 0;\) -> !VOID
  end toggle
  1..100 -> toggle -> !VOID
  $@ -> \[i](<=1> ' $i;' !\) !
end hundredDoors

$hundredDoors -> 'Open doors:$...;' -> !OUT::write
