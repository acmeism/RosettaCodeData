!yamlscript/v0

defn main():
  say: |-
    Open doors after 100 passes:
    $(open-doors().join(', '))

defn open-doors():
  ? for [d n] map(vector doors() range().drop(1))
        :when d
  : n

defn doors():
  reduce:
    fn(doors idx): doors.assoc(idx true)
    into []: repeat(100 false)
    map \(sqr(_).--): 1 .. 10
