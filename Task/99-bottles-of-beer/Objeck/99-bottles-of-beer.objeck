class Bottles {
  function : Main(args : String[]) ~ Nil {
    bottles := 99;
    do {
      "{$bottles} bottles of beer on the wall"->PrintLine();
      "{$bottles} bottles of beer"->PrintLine();
      "Take one down, pass it around"->PrintLine();
      bottles--;
      "{$bottles} bottles of beer on the wall"->PrintLine();
    } while(bottles > 0);
  }
}
