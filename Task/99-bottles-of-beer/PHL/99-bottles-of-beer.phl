module bottles;
extern printf;

@Integer main [
  @Integer bottles = 99;
  do
  {
    printf("%u bottles of beer on the wall\n", bottles);
    printf("%u bottles of beer\n", bottles);
    printf("Take one down, pass it around\n");
    bottles = bottles::dec;
    printf("%u bottles of beer on the wall\n\n", bottles);
  } while(bottles > 0);
  return 0;
]
