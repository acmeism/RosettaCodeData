use std::io;
use std::str::FromStr;

fn main() {
    let mut line = String::new();
    io::stdin().read_line(&mut line).unwrap();

    let result = line.trim()
                     .split(' ')
                     .map(|x| isize::from_str(x).unwrap())
                     .fold(0, |sum, x| sum + x);

    println!("{}", result);
}
