on new (me)
  -- if this class doesn't implement all functions of the
  -- interface class, instantiation fails
  if not implementsInterface(me, script("InterfaceClass")) then
    return VOID
  end if
  return me
end

on foo (me)
  put "FOO"
end

on bar (me)
  put "BAR"
end

on foobar (me)
  put "FOOBAR"
end
