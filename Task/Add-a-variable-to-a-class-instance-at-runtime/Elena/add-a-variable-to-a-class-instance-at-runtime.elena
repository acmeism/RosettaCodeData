import extensions.

class Extender :: BaseExtender
{
    object prop foo :: theField.

    constructor new : anObject
    [
        theObject := anObject.
    ]
}

public program =
[
    var anObject := 234.

    // extending an object with a field
    anObject := Extender new(anObject).

    anObject foo := "bar".

    console printLine(anObject,".foo=",anObject foo).

    console readChar.
].
