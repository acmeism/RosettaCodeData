proc isprime x . r .
   r = 1
   for i = 2 to sqrt x
      if x mod i = 0
         r = 0
         break 2
      .
   .
.
proc digsum n . sum .
   sum = 0
   while n > 0
      sum += n mod 10
      n = n div 10
   .
.
for i = 2 to 500
   call isprime i r
   if r = 1
      call digsum i s
      call isprime s r
      if r = 1
         write i & " "
      .
   .
.
print ""
