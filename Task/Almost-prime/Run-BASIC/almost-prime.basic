for k = 1 to 5
    print "k = "; k; " :";
    i = 2
    c = 0
    while c < 10
        if kPrime(i, k) then
            print " "; using("###", i);
            c = c +1
        end if
        i = i +1
    wend
    print
next k
end

function kPrime(n, k)
    f = 0
    for i = 2 to n
        while n mod i = 0
            if f = k then kPrime = 0
            f = f +1
            n = int(n / i)
        wend
    next i
    kPrime = abs(f = k)
end function
