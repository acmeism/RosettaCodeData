FUNCTION Fibonacci (num)
    IF num < 0 THEN
       PRINT "Invalid argument: ";
       LET Fibonacci = num
    END IF

    IF num < 2 THEN
       LET Fibonacci = num
    ELSE
       LET Fibonacci = Fibonacci(num - 1) + Fibonacci(num - 2)
    END IF
END FUNCTION

PRINT Fibonacci(20)
PRINT Fibonacci(30)
PRINT Fibonacci(-10)
PRINT Fibonacci(10)
END
