use Structure;

bundle Default {
  class Test {
    function : Main(args : String[]) ~ Nil {
      Run();
    }

    function : native : Run() ~ Nil {
      values := IntVector->New([1, 2, 3, 4, 5]);
      squares := values->Apply(Square(Int) ~ Int);
      each(i : squares) {
        squares->Get(i)->PrintLine();
      };
    }

    function : Square(value : Int) ~ Int {
      return value * value;
    }
  }
}
