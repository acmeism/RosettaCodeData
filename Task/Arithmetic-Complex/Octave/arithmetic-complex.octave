disp(z1 - z2);    % 0.0 + 1.5i
disp(z1 * z2);    % -2.25 + 6.75i
disp(z1 / z2);    % 1.5 + 0.5i
disp(-z1);        % -1.5 - 3i
disp(z1');        % 1.5 - 3i
disp(abs(z1));    % 3.3541 = sqrt(z1*z1')
