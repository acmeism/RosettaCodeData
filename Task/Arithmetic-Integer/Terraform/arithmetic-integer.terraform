#Aamrun, 15th August 2022

variable "a" {
  type = number
}

variable "b" {
  type = number
}

output "Sum" {
  value = var.a + var.b
}

output "Difference" {
  value = var.a - var.b
}

output "Product" {
  value = var.a * var.b
}

output "Quotient" {
  value = floor(var.a / var.b)
}

output "Remainder" {
  value = var.a % var.b
}
