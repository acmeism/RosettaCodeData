Imports System.Console
Module Module1
  Sub Main
    Dim a = CInt(ReadLine)
    Dim b = CInt(ReadLine)
    WriteLine("Sum " & a + b)
    WriteLine("Difference " & a - b)
    WriteLine("Product " & a - b)
    WriteLine("Quotient " & a / b)
    WriteLine("Integer Quotient " & a \ b)
    WriteLine("Remainder " & a Mod b)
    WriteLine("Exponent " & a ^ b)
  End Sub
End Module
