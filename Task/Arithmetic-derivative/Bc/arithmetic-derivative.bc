define d(n) {
    auto f
    if(n<0) return -d(-n)
    if(n<2) return 0
    s = 0
    r = n
    for(f=2;f<=r;f++) {
        while(r%f==0){
            r /= f
            s += n / f
        }
    }
    return s
}

define padl(n,f) {
    for(i=0;i<f-(length(n)+(n<0));i++)
        print " "
    print n
}

