pi[n_, prec_] :=
 Module[{a = 1, g = N[1/Sqrt[2], prec], k, s = 0, p = 4},
  For[k = 1, k < n, k++,
   {a, g} = {N[(a + g)/2, prec], N[Sqrt[a g], prec]};
   s += p (a^2 - g^2); p += p]; N[4 a^2/(1 - s), prec]]


