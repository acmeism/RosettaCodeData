$ include "seed7_05.s7i";

var array integer: a is [] (1, 2, 3, 4);
var array integer: b is [] (5, 6, 7, 8);
var array integer: c is [] (9, 10);

const proc: main is func
  local
    var integer: number is 0;
  begin
    c := a & b;
    for number range c do
      write(number <& " ");
    end for;
    writeln;
  end func;
