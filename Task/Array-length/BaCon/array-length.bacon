' Static arrays

DECLARE fruit$[] = { "apple", "orange" }
PRINT UBOUND(fruit$)

' Dynamic arrays

DECLARE vegetable$ ARRAY 2
vegetable$[0] = "cabbage"
vegetable$[1] = "spinach"
PRINT UBOUND(vegetable$)

' Associative arrays

DECLARE meat$ ASSOC STRING
meat$("first") = "chicken"
meat$("second") = "pork"
PRINT UBOUND(meat$)
