COMMENT ARRAY-LENGTH;
BEGIN

    INTEGER PROCEDURE ARRAYLENGTH(A); TEXT ARRAY A;
    BEGIN
        ARRAYLENGTH := UPPERBOUND(A, 1) - LOWERBOUND(A, 1) + 1;
    END ARRAYLENGTH;

    TEXT ARRAY A(1:2);
    INTEGER L;
    A(1) :- "APPLE";
    A(2) :- "ORANGE";
    L := ARRAYLENGTH(A);
    OUTINT(L, 0);
    OUTIMAGE;
END
