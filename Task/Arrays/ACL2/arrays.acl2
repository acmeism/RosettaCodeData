;; Create an array and store it in array-example
(assign array-example
        (compress1 'array-example
                   (list '(:header :dimensions (10)
                                   :maximum-length 11))))

;; Set a[5] to 22
(assign array-example
        (aset1 'array-example
               (@ array-example)
               5
               22))

;; Get a[5]
(aref1 'array-example (@ array-example) 5)
