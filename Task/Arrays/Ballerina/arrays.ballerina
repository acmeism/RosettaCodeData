import ballerina/io;

public function main() {
    // fixed length array
    int[4] a = [1, 4, 9, 16];
    // retrieve and print element at index 1 (zero indexing)
    io:println(a[1]);

    // dynamic array
    int[] b = [];
    // push an element into it
    b.push(42);
    // retrieve and print last element added
    io:println(b[b.length() - 1]);
}
