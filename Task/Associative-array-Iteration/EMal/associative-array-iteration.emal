Map map = text%text["Italy" => "Rome", "France" => "Paris"]
map.insert("Germany", "Berlin")
map["Spain"] = "Madrid"
writeLine("== pairs ==")
for each Pair pair in map
  writeLine(pair)
end
writeLine("== keys ==")
for each text key in map.keys()
  writeLine(key)
end
writeLine("== values ==")
for each text value in map.values()
  writeLine(value)
end
