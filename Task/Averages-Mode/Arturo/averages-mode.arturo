getMode: function [arr][
    freqs: new #[]
    loop arr 'i [
        k: to :string i
        if not? key? freqs k -> set freqs k 0
        freqs\[k]: (freqs\[k]) + 1
    ]
    maximum: max values freqs
    select keys freqs 'i -> maximum = freqs\[i]
]

print getMode [1 3 6 6 6 6 7 7 12 12 17]
print getMode [1 1 2 4 4]
