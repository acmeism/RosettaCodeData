  [] 10 times [ i^ 1+ join ]

  say "Arithmetic mean:" sp
  0 over witheach +
  over size 8 point$ echo$
  cr
  say " Geometric mean:" sp
  1 over witheach *
  over size 80 ** * 10 root
  10 8 ** 8 point$ echo$
  cr
  say "  Harmonic mean:" sp
  dup size dip
    [ 0 n->v rot
      witheach [ n->v 1/v v+ ] ]
  n->v 2swap v/ 8 point$ echo$
