test := ""
tek := 1, bases := " "
loop,parse,test
	{
	if (A_LoopField = "A")
		countA += 1
	else if (A_LoopField = "C")
		countC += 1
	else if (A_LoopField = "G")
		countG += 1
	else if (A_LoopField = "T")
		countT += 1
	if (mod(a_index,50) = 0)
		{
			bases .= a_index . " -> " . substr(test,tek,50) . "`n"
			tek += 50
		}
	}
MsgBox % bases "`nA: " countA "`nC: " countC "`nG: " countG "`nT: " countT "`nTotal = " countA+countC+countG+countT
ExitApp
