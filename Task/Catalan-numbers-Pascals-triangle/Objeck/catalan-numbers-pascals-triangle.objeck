class Test {
  function : Main(args : String[]) ~ Nil {
    N := 15;
    t := Int->New[N + 2];
    t[1] := 1;

    for(i := 1; i <= N; i++) {

      for(j := i; j > 1; j--){
        t[j] := t[j] + t[j - 1];
      };

      t[i + 1] := t[i];

      for(j := i + 1; j > 1; j--) {
        t[j] := t[j] + t[j - 1];
      };

      v := t[i + 1] - t[i];
      "{$v} "->Print();
    };
  }
}
