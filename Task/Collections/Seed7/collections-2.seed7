$ include "seed7_05.s7i";

const proc: main is func
  local
    var array string: anArray is [] ("iron", "copper");
    var string: element is "";
  begin
    for element range anArray do
      write(element <& " ");
    end for;
    writeln;
    anArray &:= "silver";
    for element range anArray do
      write(element <& " ");
    end for;
    writeln;
  end func;
