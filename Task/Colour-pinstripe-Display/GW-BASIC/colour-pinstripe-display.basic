10 SCREEN 8
20 CLS
30 W = 640: H = 200
40 H = H \ 4
50 Y2 = H - 1
60 FOR I = 1 TO 4
70   COL = 0
80   Y = (I - 1) * H
90   FOR X = 1 TO W STEP I
100     IF COL MOD 15 = 0 THEN COL = 0
110     LINE (X, Y)-(X + I, Y + H), COL, BF
120     COL = COL + 1
130   NEXT X
140 NEXT I
150 WHILE INKEY$= "": WEND
160 END
