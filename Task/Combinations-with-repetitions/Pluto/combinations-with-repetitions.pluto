require "perm"
local fmt = require "fmt"

local function rep(a, multiple)
    local aa = {}
    for i = 1, #a do
         for _ = 1, multiple do aa:insert(a[i]) end
    end
    return aa
end

local a = {"iced", "jam", "plain"}
fmt.lprint(comb.list(rep(a, 2), 2, true))
a = range(1, 10)
print(#comb.list(rep(a, 3), 3, true))
