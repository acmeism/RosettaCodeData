func isSorted(xs) {
    var prev
    for x in xs {
        if prev && !(x > prev) {
            return false
        }
        prev = x
    }
    true
}

func isEqual(xs) {
    var prev
    for x in xs {
        if prev && x != prev {
            return false
        }
        prev = x
    }
    true
}
