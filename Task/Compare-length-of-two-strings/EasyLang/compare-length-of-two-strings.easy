proc stringLength a$ b$ . .
   lineA$ = "\"" & a$ & "\" (length: " & len a$ & ")"
   lineB$ = "\"" & b$ & "\" (length: " & len b$ & ")"
   if len a$ >= len b$
      print lineA$ ; print lineB$
   else
      print lineB$ ; print lineA$
   .
.
call stringLength "Easy" "Language"
call stringLength "Rosetta" "Code"
