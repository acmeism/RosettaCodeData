PROC main:
  LOCAL string1$(7),string2$(4),length1%,length2%
  string1$="Rosetta"
  string2$="Code"
  length1%=LEN(string1$)
  length2%=LEN(string2$)
  IF length1%>length2%
    PRINT string1$,"(";length1%;")"
    PRINT string2$,"(";length2%;")"
  ELSE
    PRINT string2$,"(";length2%;")"
    PRINT string1$,"(";length1%;")"
  ENDIF
  GET
ENDP
