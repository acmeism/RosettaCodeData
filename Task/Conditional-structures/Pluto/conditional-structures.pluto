local val = 1
if val > 2 then
   print("a")
elseif val == 2 then
   print("b")
else
   print("c")
end

val = 123
switch val do
   case 1: print(1); break
   case 2: print(2); break
   default: print(123)
end

-- Hash-based conditionals
local t = {
   ["one"] = function() print("one") end,
   ["two"] = function() print("two") end
}
-- (default)
setmetatable(t, {
  __index = function(_) print("default") return function() end end
})

t["one"]()
t["two"]()
t["100000"]()
