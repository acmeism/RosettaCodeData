BEGIN
  INTEGER i,j;
  i:=1; j:=2;
  OutText("i ");
  IF i=1 THEN OutInt(i,1);
  OutImage;
  OutInt(i,2); OutInt(j,2);
  IF i<j THEN OutText(" : i<j") ELSE OutText(" : i>=j");
  OutImage;
  IF i>=j THEN BEGIN
    OutText("i=");
    OutInt(i,5)
  END
  ELSE BEGIN
    OutText("j=");
    OutInt(j,5)
  END;
  OutImage
END
