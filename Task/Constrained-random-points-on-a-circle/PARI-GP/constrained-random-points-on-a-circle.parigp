crpc()={
  my(v=vector(404),t=0,i=0,vx=vy=vector(100));
  for(x=1,14,for(y=1,14,
    t=x^2+y^2;
    if(t>99&t<226,
      v[i++]=[x,y];
      v[i++]=[x,-y];
      v[i++]=[-x,y];
      v[i++]=[-x,-y]
    )
  ));
  for(x=10,15,
    v[i++]=[x,0];
    v[i++]=[-x,0];
    v[i++]=[0,x];
    v[i++]=[0,-x]
  );
  for(i=1,#vx,
    t=v[random(#v)+1];
    vx[i]=t[1];
    vy[i]=t[2];
  );
  plothraw(vx,vy)
};
