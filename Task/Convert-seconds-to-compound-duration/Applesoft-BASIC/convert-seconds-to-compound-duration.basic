100 DATA604800,WK,86400,D,3600,HR,60,MIN,1,SEC
110 FOR I = 0 TO 4
120     READ M(I), U$(I)
130 NEXT
140 DATA7259,86400,6000000
150 ON ERR GOTO 270
160     READ S
170     GOSUB 200
180     PRINT S " = " S$
190 GOTO 160

200 N = S
210 S$ = ""
220 FOR I = 0 TO 4
230     IF INT(N / M(I)) THEN S$ = S$ + MID$(", ", 1, (LEN(S$) > 0) * 2) + STR$(INT(N / M(I))) + " " + U$(I)
240     N = N - INT(N / M(I)) * M(I)
250 NEXT I
260 RETURN

270 END
