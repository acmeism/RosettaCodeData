Module CheckArray {
      Do {
            Input "A, B=", A% ,B%
      } Until A%>0 and B%>0

      \\ 1@ is 1 Decimal
      addone=lambda N=1@ ->{=N : N++}
      Dim Base 1, Arr(A%,B%)<<addone()
      \\ pi also is decimal
      Arr(1,1)=pi
      Print Arr(1,1)
      Print Arr()
      \\ all variables/arrays/inner functions/modules erased now
}
CheckArray
