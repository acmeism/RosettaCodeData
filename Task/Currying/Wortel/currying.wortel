@let {
  addOne \+ 1
  subtractFrom1 \- 1
  subtract1 \~- 1

  subtract1_2 &\- [. 1]

  add ^+
  ; partial apply to named functions
  addOne_2 \add 1

  ; testing
  [[
    !addOne 5 ; returns 6
    !subtractFrom1 5 ; returns -4
    !subtract1 5 ; returns 4
    !subtract1_2 5 ; returns 4
    !addOne_2 5 ; returns 6
  ]]
}
