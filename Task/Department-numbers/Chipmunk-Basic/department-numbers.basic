10 CLS
20 PRINT "--police--  --sanitation--  --fire--"
30 FOR police = 2 TO 7 STEP 2
40    FOR fire = 1 TO 7
50        sanitation = 12-police-fire
60        IF sanitation >= 1 AND sanitation <= 7 THEN
70            PRINT TAB (5)police TAB (18)fire TAB (30)sanitation
80        endif
90    NEXT fire
100 NEXT police
