require "list"

local d = dlist.of("A", "B")
print(d)
-- Add an element at the head.
d:prepend("C")
-- Add an element in the middle.
d:insert(2, "D")
-- Add an element at the tail.
d:push("E")
print(d)

-- Remove an element by index (indexing is 1 based).
d:remove(2)
print(d)
-- Remove an element by value.
d:erase("A")
print(d)
