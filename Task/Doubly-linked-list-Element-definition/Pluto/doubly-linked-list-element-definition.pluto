require "list"

local s = dlist.of(1, 2, 3)
local n1 = s:node(1)
local n2 = s:node(2)
local n3 = s:node(3)
print($"node 1: value = {n1.value}, next = {tostring(n1.next?.value)}, prev = {tostring(n1.prev?.value)}")
print($"node 2: value = {n2.value}, next = {tostring(n2.next?.value)}, prev = {tostring(n2.prev?.value)}")
print($"node 3: value = {n3.value}, next = {tostring(n3.next?.value)}, prev = {tostring(n3.prev?.value)}")
