%!PS-Adobe-3.0
%%BoundingBox 0 0 300 300

/+ { 90 rotate } def
/- {-90 rotate } def
/!1 { dup 1 sub dup 0 eq not } def

/F { 180 0 rlineto } def
/X { !1 { X + Y F + } if pop } def
/Y { !1 { - F X - Y } if pop } def

/dragon {
        gsave
        70 180 moveto
        dup 1 sub { 1 2 div sqrt dup scale -45 rotate } repeat
        F X stroke
        grestore
} def

1 1 20 { dragon showpage } for

%%EOF
