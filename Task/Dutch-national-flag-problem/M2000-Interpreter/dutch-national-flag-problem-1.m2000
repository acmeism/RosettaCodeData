	SortElse=lambda white (a(), &swaps, &compares)->{
		long mid=white
		long i, j, k=len(a())-1
		while j<=k
			
			if a(j)<mid then
				compares++
				swap a(i), a(j)
				swaps++
				i++
				j++
			else.if a(j)>mid then
				compares+=2
				swap a(j), a(k)
				swaps++
				k--
			else
				compares+=2
				j++
			end if
		end while
		=a()
	}
