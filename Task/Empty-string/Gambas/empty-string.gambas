Public Sub Main()
Dim sString As String[] = ["", "Hello", "world", "", "Today", "Tomorrow", "", "", "End!"]
Dim sTemp As String
Dim siCount As Short

For Each sTemp In sString
  If sString[siCount] Then
    Print "String " & siCount & " = " & sString[siCount]
  Else
    Print "String " & siCount & " is empty"
  End If
  Inc siCount
Next

End
