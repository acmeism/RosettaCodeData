10  REM Ethiopian multiplication
20  DEF FNE(A%) = (A% + 1) MOD 2
30  DEF FNH(A%) = A% \ 2
40  DEF FND(A%) = 2 * A%
50  X% = 17: Y% = 34: TOT% = 0
60  WHILE X% >= 1
70   PRINT USING "###### "; X%;
80   IF FNE(X%)=0 THEN TOT% = TOT% + Y%: PRINT USING "###### "; Y% ELSE PRINT
90   X% = FNH(X%): Y% = FND(Y%)
100 WEND
110 PRINT USING "=      ######"; TOT%
120 END
