Function ethiopian(mr as long long, md as long long) {
	def even()=number mod 2&&
	def div2()=number div 2&&
	def mul2()=number * 2&&
	result=0&&
	while mr>=1
		if even(mr) then result+=md
		mr=div2(mr)
		md=mul2(md)
	end while
	=result
}
Print ethiopian(17, 34)=578
Function ethiopian(mr as long long, md as long long) {
