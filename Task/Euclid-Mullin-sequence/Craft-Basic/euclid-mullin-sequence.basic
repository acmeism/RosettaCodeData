define size = 16, em = 0
dim list[size]

let list[0] = 2
print 2

for i = 1 to 15

	let k = 3

	do

		let em = 1

		for j = 0 to i - 1

			let em = ( em * list[j] ) % k

		next j

		let em = ( em + 1 ) % k

		if em = 0 then

			let list[i] = k
			print list[i]
			break

		endif

		let k = k + 2

		wait

	loop

next i

print "done."

end
