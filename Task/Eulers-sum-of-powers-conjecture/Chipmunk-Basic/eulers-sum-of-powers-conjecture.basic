100 max = 250
110 for w = 1 to max
120  for x = 1 to w
130   for y = 1 to x
140    for z = 1 to y
150     sum = w^5+x^5+y^5+z^5
160     sol = int(sum^0.2)
170     if sum = sol^5 then
180      print w;"^5 + ";x;"^5 + ";y;"^5 + ";z;"^5 = ";sol;"^5"
190      end
200     endif
210    next z
220   next y
230  next x
240 next w
