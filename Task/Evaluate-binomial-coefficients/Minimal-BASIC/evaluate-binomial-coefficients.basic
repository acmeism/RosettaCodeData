10 REM MAIN PROGRAM
20 FOR N = 0 TO 14
30 FOR K = 0 TO N
40 GOSUB 100
50 PRINT B
60 NEXT K
70 PRINT
80 NEXT N
90 GOTO 999
100 REM SUB BINOMIAL
110 IF K = 0 THEN 180
120 IF K = N THEN 180
130 IF K < 0 THEN 190
140 IF K > N THEN 190
150 LET P = 1
160 FOR I = 1 TO K
170 LET P = P * (N-K+I)/I
175 NEXT I
177 LET B = P
178 RETURN
180 LET B = 1
185 RETURN
190 LET B = 0
195 RETURN
999 END
