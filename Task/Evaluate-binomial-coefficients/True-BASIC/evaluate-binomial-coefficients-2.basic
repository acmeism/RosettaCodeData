    REM PASCAL'S TRIANGLE
    LET N=0
 10 LET K=0
 20 GOSUB 30
    PRINT B," "
    LET K=K+1
    IF K<=N THEN GOTO 20
    PRINT ""
    LET N=N+1
    IF N<=10 THEN GOTO 10
    END
 30 REM BINOMIAL
    IF K=0 THEN GOTO 50
    IF K=N THEN GOTO 50
    LET P=1
    LET I=1
 40 LET T=N-K+I
    LET P=P*T/I
    LET I=I+1
    IF I<=K THEN GOTO 40
    LET B=P
    RETURN
 50 LET B=1
    RETURN
	
