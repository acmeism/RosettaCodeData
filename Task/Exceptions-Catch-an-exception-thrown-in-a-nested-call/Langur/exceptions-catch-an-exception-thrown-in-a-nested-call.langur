val U0 = {"msg": "U0"}
val U1 = {"msg": "U1"}

val baz = fn i: throw if(i==0: U0; U1)
val bar = fn i: baz(i)

val foo = impure fn() {
    for i in [0, 1] {
        bar(i)
        catch {
	        if _err'msg == U0'msg {
	            writeln "caught U0 in foo()"
	        } else {
	            throw
	        }
		}
    }
}

foo()
