proc run code$ .
   for c$ in strchars code$
      if c$ = "Q"
         print code$
      elif c$ = "H"
         print "Hello, world!"
      elif c$ = "9"
         for b = 99 downto 1
            print b & " bottles of beer on the wall"
            print b & " bottles of beer"
            print "Take one down, pass it around"
            print b & " bottles of beer on the wall"
            print ""
         .
      elif c$ = "+"
         acc += 1
         print acc
      .
   .
.
run "HQ9+"
