Public Sub Main()

  Print "     x     p    |     -x^p      -(x)^p      (-x)^p    -(x^p)"
  Print "----------------+---------------------------------------------"
  For x As Integer = -5 To 5 Step 10
    For p As Integer = 2 To 3
        Print "    "; Format$(x, "-#"); "    "; Format$(p, "-#"); "    |     "; Format$((-x ^ p), "-###"); "       "; Format$((-(x) ^ p), "-###"); "        "; Format$(((-x) ^ p), "-###"); "      "; Format$((-(x ^ p)), "-###")
    Next
  Next

End
