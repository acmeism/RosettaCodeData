PRINT "     x     p    |     -x^p      -(x)^p      (-x)^p    -(x^p)"
PRINT "----------------+---------------------------------------------"
FOR x = -5 TO 5 STEP 10
    FOR p = 2 TO 3
        PRINT  USING "    ##    ##    |    ####       ####       ####       ####": x, p, (-x^p), (-(x)^p), ((-x)^p), (-(x^p))
    NEXT p
NEXT x
END
