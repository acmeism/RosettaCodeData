select x, typeof(x), x + 0.0, x - x
from unnest([NULL::FLOAT, 'Infinity'::FLOAT, '-Infinity'::FLOAT, 'NaN'::FLOAT, '-NaN'::FLOAT]) _(x);

select x, typeof(x), x + 0.0, x - x
from unnest([NULL::DOUBLE, 'Infinity'::DOUBLE, '-Infinity'::FLOAT,'NaN'::DOUBLE, '-NaN'::DOUBLE]) _(x);

select x, x + 1::INTEGER, x = x, x is null
from unnest([NULL::DOUBLE, 'Infinity'::DOUBLE, '-Infinity'::FLOAT,'NaN'::DOUBLE, '-NaN'::DOUBLE]) _(x);
