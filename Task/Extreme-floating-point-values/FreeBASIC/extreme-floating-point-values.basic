#Include "crt/math.bi"

Dim inf     As Double = INFINITY
Dim negInf  As Double = -INFINITY
Dim notNum  As Double = NAN_
Dim negZero As Double = 1.0 / negInf

Print inf,     inf / inf
Print negInf,  negInf * negInf
Print notNum,  notNum + inf + negInf
Print negZero, negZero - 1
Sleep
