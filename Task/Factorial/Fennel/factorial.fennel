(do
    (fn factorial [n]    ;;; iterative factorial
        (if (< n 2)
            1
        ;else
            (do (var f 2)
                (for [i 3 n]
                     (set f (* f i))
                )
                f
            )
        )
    )

    (for [n 0 10]
         (io.write (string.format "%2d! = %s\n" n (factorial n)))
    )
)
