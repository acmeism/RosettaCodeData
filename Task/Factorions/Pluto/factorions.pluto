-- Cache factorials from 0 to 11.
local fact = {[0] = 1}
for n = 1, 11 do fact[n] = fact[n - 1] * n end

for b = 9, 12 do
    print($"The factorions for base {b} are:")
    for i = 1, 1_499_999 do
        local sum  = 0
        local j = i
        while j > 0 do
            local d = j % b
            sum += fact[d]
            j //= b
        end
        if sum == i then io.write($"{i} ") end
    end
    print("\n")
end
