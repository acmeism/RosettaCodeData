mersenneFactors: function [q][
    if not? prime? q -> print "number not prime!"
    r: new q
    while -> r > 0
          -> shl 'r 1
    d: new 1 + 2 * q
    while [true][
        i: new 1
        p: new r
        while [p <> 0][
            i: new (i * i) % d
            if p < 0 -> 'i * 2
            if i > d -> 'i - d
            shl 'p 1
        ]
        if? i <> 1 -> 'd + 2 * q
        else -> break
    ]
    print ["2 ^" q "- 1 = 0 ( mod" d ")"]
]

mersenneFactors 929
