Sub printFactors(n As Integer)
  If n < 1 Then Return
  Print n; " =>";
  For i As Integer = 1 To n / 2
    If n Mod i = 0 Then Print i; " ";
  Next i
  Print n
End Sub

printFactors(11)
printFactors(21)
printFactors(32)
printFactors(45)
printFactors(67)
printFactors(96)
Print
Print "Press any key to quit"
Sleep
