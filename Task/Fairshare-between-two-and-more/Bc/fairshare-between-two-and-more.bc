define fairshare(n, base) {
    auto result
    while (n>0) {
        result += n % base
        n /= base
    }
    return result % base
}

define help(base) {
    print base, ": "
    for(i=0;i<25;i++)
        print fairshare(i, base), " "
    print "\n"
}

{ q=help(2)
  q=help(3)
  q=help(5)
  q=help(11)
}
