%!PS

/Courier 16 selectfont

/fibonacci {
  dup 2 lt
  { }
  {
    0 exch
    1 exch
    2 exch 1 exch {
      pop
      exch
      1 index
      add
    } for
    exch
    pop
  } ifelse
} def

/str 4 string def

/n 10 def

72 720 moveto

0 1 n {
  fibonacci str cvs
  str show
  pop
} for

showpage
