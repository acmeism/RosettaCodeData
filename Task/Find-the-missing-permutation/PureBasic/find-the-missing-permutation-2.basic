If OpenConsole()
  NewList a.s()
  findPermutations(a(), "ABCD", 4)
  ForEach a()
    Select a()
      Case "ABCD","CABD","ACDB","DACB","BCDA","ACBD","ADCB","CDAB","DABC"
      Case "BCAD","CADB","CDBA","CBAD","ABDC","ADBC","BDCA","DCBA","BACD"
      Case "BADC","BDAC","CBDA","DBCA","DCAB"
      Default
        PrintN(A()+" is missing.")
    EndSelect
  Next

  Print(#CRLF$ + "Press ENTER to exit"): Input()
EndIf
