fivenum: function [lst][
    lst: sort lst
    m: (size lst)/2
    lowerEnd: (odd? size lst)? -> m -> m-1

    return @[
        first lst
        median slice lst 0 lowerEnd
        median slice lst 0 dec size lst
        median slice lst m dec size lst
        last lst
    ]
]

