MODULE Fizzbuzz EXPORTS Main;

IMPORT IO;

BEGIN
   FOR i := 1 TO 100 DO
      IF i MOD 15 = 0 THEN
         IO.Put("FizzBuzz\n");
      ELSIF i MOD 5 = 0 THEN
         IO.Put("Buzz\n");
      ELSIF i MOD 3 = 0 THEN
         IO.Put("Fizz\n");
      ELSE
         IO.PutInt(i);
         IO.Put("\n");
      END;
   END;
END Fizzbuzz.
