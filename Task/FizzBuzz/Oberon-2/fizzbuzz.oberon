MODULE FizzBuzz;

   IMPORT Out;

   VAR i: INTEGER;

BEGIN
   FOR i := 1 TO 100 DO
      IF i MOD 15 = 0 THEN
         Out.String("FizzBuzz")
      ELSIF i MOD 5 = 0 THEN
         Out.String("Buzz")
      ELSIF i MOD 3 = 0 THEN
         Out.String("Fizz")
      ELSE
         Out.Int(i,0)
      END;
      Out.Ln
   END
END FizzBuzz.
