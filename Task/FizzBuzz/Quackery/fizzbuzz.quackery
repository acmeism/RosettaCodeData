  [ times
      [ i^ 1+
        ' echo
        over 3 mod 0 = if
          [ say "Fizz"
            drop ' drop ]
        over 5 mod 0 = if
          [ say "Buzz"
            drop ' drop ]
        do
        sp ]
    cr ]                  is fizzbuzz ( n --> )

  say 'First 100 turns in the game of fizzbuzz:' cr cr
  100 fizzbuzz cr
