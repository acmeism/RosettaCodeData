{def gapfuls
 {lambda {:n :i :N}
  {if {>= :i :N}
   then
   else {if {= {% :n {W.first :n}{W.last :n}} 0}
   then :n {gapfuls {+ :n 1} {+ :i 1} :N}
   else    {gapfuls {+ :n 1}    :i    :N}}}}}
-> gapfuls

