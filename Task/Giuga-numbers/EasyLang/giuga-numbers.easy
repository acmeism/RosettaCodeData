proc giuga m . r .
   n = m
   r = 0
   for f = 2 to sqrt n
      while n mod f = 0
         if (m div f - 1) mod f <> 0
            break 2
         .
         n = n div f
         if f > n
            r = 1
            break 2
         .
      .
   .
.
n = 3
while cnt < 4
   call giuga n r
   if r = 1
      cnt += 1
      print n
   .
   n += 1
.
