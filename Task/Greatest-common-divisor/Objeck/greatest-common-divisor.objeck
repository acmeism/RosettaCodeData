bundle Default {
  class GDC {
    function : Main(args : String[]), Nil {
      for(x := 1; x < 36; x += 1;) {
        IO.Console->GetInstance()->Print("GCD of ")->Print(36)->Print(" and ")->Print(x)->Print(" is ")->PrintLine(GDC(36, x));
      };
    }

    function : native : GDC(a : Int, b : Int), Int {
      t : Int;

      if(a > b) {
        t := b;  b := a;  a := t;
      };

      while (b <> 0) {
        t := a % b;  a := b;  b := t;
      };

      return a;
    }
  }
}
