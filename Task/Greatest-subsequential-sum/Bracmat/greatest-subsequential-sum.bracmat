(   0:?max
  & :?seq
  &   -1 -2 3 5 6 -2 -1 4 -4 2 -1
    :   ?
        [%(   (
              =   s sum
                .   ( sum
                    =   A
                      .   !arg:%?A ?arg&!A+sum$!arg
                        | 0
                    )
                  & (   sum$!sjt:>!max:?max
                      & !sjt:?seq
                    |
                    )
              )
            $
          & ~
          )
        ?
| !seq
)
