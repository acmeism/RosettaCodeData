for i = 1 to 4
	incr	= int(256 / (i * 8))
	c	= 256
        html "<table style='width: 200px; height: 11px;' border=0 cellpadding=0 cellspacing=0><tr>"
	for j = 1 to i * 8
		html "<td style='background-color: rgb(";c;",";c;",";c;");'></td>"
		c = c - incr
	next j
        html "</tr>"
next i
html "</table>"
end
