writeln "Guess a number from 1 to 10"

val .n = toString random 10
for {
    val .guess = read ">> ", RE/^0*(?:[1-9]|10)(?:\.0+)?$/, "bad data\n", 7, ""
    if .guess == "" {
        writeln "too much bad data"
        break
    }
    if .guess == .n {
        writeln "That's it."
        break
    }
    writeln "not it"
}
