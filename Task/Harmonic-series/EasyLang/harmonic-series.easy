numfmt 2 5
print "The first twenty harmonic numbers are:"
for n = 1 to 20
   h += 1 / n
   print n & " " & h
.
print ""
print "The first harmonic number greater than: "
h = 1
n = 2
for i = 2 to 10
   while h < i
      h += 1 / n
      n += 1
   .
   print i & " is " & h & ", at position " & n - 1
.
