dim as double h = 0.0
dim as uinteger n, i

print "The first twenty harmonic numbers are:"
for n = 1 to 20
    h += 1.0/n
    print n, h
next n

h = 1 : n = 2
for i=2 to 10
    while h<i
        h+=1.0/n
        n+=1
    wend
    print "The first harmonic number greater than ";i;" is ";h;", at position ";n-1
next i
