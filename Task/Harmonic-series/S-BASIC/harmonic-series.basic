var i, n = integer
var h = real.double

print "First 20 harmonic numbers:"
h = 0
for i = 1 to 20
   h = h + 1 / i
   print using "##  #.######"; i; h
next i

for i = 1 to 5
  h = 1
  n = 2
  while h <= i do
    begin
      h = h + 1 / n
      n = n + 1
    end
  print "First term >"; i; " is at position"; n-1
next i

end
