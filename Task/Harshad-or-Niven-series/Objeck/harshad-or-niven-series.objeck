class Harshad {
  function : Main(args : String[]) ~ Nil {
    count := 0;
    for(i := 1; count < 20; i += 1;) {
      if(i % SumDigits(i) = 0){
        "{$i} "->Print();
        count += 1;
      };
    };

    for(i := 1001; true; i += 1;) {
      if(i % SumDigits(i) = 0){
        "... {$i}"->PrintLine();
        break;
      };
    };
  }

  function : SumDigits(n : Int) ~ Int {
    sum := 0;
    do {
      sum += n % 10;
      n /= 10;
    } while(n <> 0);

    return sum;
  }
}
