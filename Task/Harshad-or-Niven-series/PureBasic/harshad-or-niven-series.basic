If OpenConsole()=0 : End 1 : EndIf

Procedure.i Niven(v.i)
  w=v
  While v : s+v%10 : v/10 : Wend
  If w%s=0 : ProcedureReturn w : EndIf
EndProcedure

Repeat
  i+1
  If Niven(i) : c+1 : Print(Str(i)+" ") : EndIf
  If c=20 And i<1000 : Print("... ") : i=1000 : EndIf
  If c=21 : Break : EndIf
ForEver

Input()
