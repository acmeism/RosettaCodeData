      INSTALL @lib$+"CLASSLIB"

      DIM Animal{method}
      PROC_class(Animal{})

      DIM Cat{method}
      PROC_inherit(Cat{}, Animal{})
      PROC_class(Cat{})

      DIM Dog{method}
      PROC_inherit(Dog{}, Animal{})
      PROC_class(Dog{})

      DIM Labrador{method}
      PROC_inherit(Labrador{}, Dog{})
      PROC_class(Labrador{})

      DIM Collie{method}
      PROC_inherit(Collie{}, Dog{})
      PROC_class(Collie{})
