100 PROGRAM "Type.bas"
110 TEXT 80
120 INPUT PROMPT "File name: ":F$
130 WHEN EXCEPTION USE IOERROR
140   OPEN #1:F$ ACCESS INPUT
150   DO
160     LINE INPUT #1,IF MISSING EXIT DO:F$
170     PRINT F$
180   LOOP
190   CLOSE #1
200 END WHEN
210 HANDLER IOERROR
220   PRINT EXSTRING$(EXTYPE)
230   END
240 END HANDLER
