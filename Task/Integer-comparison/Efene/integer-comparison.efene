compare = fn (A, B) {
    if A == B {
        io.format("~p equals ~p~n", [A, B])
    }
    else {
        ok
    }

    if A < B {
        io.format("~p is less than ~p~n", [A, B])
    }
    else {
        ok
    }

    if A > B {
        io.format("~p is greater than ~p~n", [A, B])
    }
    else {
        ok
    }
}

@public
run = fn () {
    compare(5, 5)
    compare(6, 5)
    compare(4, 5)
}
