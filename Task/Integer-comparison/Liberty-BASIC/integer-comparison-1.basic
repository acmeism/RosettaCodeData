input "Enter an integer for a.  ";a
input "Enter an integer for b.  ";b

'a=int(a):b=int(b) ???
print "Conditional evaluation."
if a<b then print "a<b " ; a ; " < " ; b
if a=b then print "a=b " ; a ; " = " ; b
if a>b then print "a>b " ; a ; " > " ; b

print "Select case evaluation."
select case
    case (a<b)
        print "a<b " ; a ; " < " ; b
    case (a=b)
        print "a=b " ; a ; " = " ; b
    case (a>b)
        print "a>b " ; a ; " > " ; b
end select
