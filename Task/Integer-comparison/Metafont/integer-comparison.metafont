message "integer 1: ";
a1 := scantokens readstring;
message "integer 2: ";
a2 := scantokens readstring;
if a1 < a2:
  message decimal a1 & " is less than " & decimal a2
elseif a1 > a2:
  message decimal a1 & " is greater than " & decimal a2
elseif a1 = a2:
  message decimal a1 & " is equal to " & decimal a2
fi;
end
