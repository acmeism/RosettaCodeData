MODULE IntCompare;

IMPORT InOut;

VAR
  A, B: INTEGER;

BEGIN
  InOut.ReadInt(A);
  InOut.ReadInt(B);
  InOut.WriteInt(A, 1);

  IF A < B THEN
    InOut.WriteString(' is less than ')
  ELSIF A = B THEN
    InOut.WriteString(' is equal to ')
  ELSE
    InOut.WriteString(' is greater than ')
  END;
  InOut.WriteInt(B, 1);
  InOut.WriteLn
END IntCompare.
