uses console
print "Integer comparison. (press ctrl-c to exit) " cr cr
int n1,n2
do
  print "Enter 1st integer " : n1=input
  print "Enter 2nd integer " : n2=input
  if n1=n2
    print "number1 = number2" cr cr
  elseif n1>n2
    print "number1 > number2" cr cr
   elseif n1<n2
    print "number1 < number2" cr cr
  endif
loop
