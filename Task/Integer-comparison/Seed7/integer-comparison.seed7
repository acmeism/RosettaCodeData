$ include "seed7_05.s7i";

const proc: main is func
  local
    var integer: a is 0;
    var integer: b is 0;
  begin
    readln(a);
    readln(b);

    if a < b then
      writeln(a <& " is less than " <& b);
    end if;

    if a = b then
      writeln(a <& " is equal to " <& b);
    end if;

    if a > b then
      writeln(a <& " is greater than " <& b);
    end if;
  end func;
