do  -- Jewels and Stones - translated from the Lua sample, using a few of Pluto's new features
    local function count_jewels(s, j)
        local count = 0
        for i=1,#s do
            if j:match(s[i]) then
                ++ count
            end
        end
        return count
    end
    io.write( $"{count_jewels('aAAbbbb', 'aA')} {count_jewels('ZZ', 'z')}\n" )
end
