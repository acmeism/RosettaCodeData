BOOL local fn JortSort( array as CFArrayRef )
end fn = fn ArrayIsEqual( array, fn ArraySortedArrayUsingSelector( array, @"compare:" ) )

print fn JortSort( @[@"d",@"a",@"b",@"e",@"f",@"c"] )
print fn JortSort( @[@10,@13,@34,@52,@101] )
print fn JortSort( @[@"Kilo",@"Hotel",@"Juliet",@"India"] )

HandleEvents
