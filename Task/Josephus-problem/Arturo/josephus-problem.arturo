josephus: function [n,k][
    p: new 0..n-1
    i: 0
    seq: []

    while [0 < size p][
        i: (i+k-1) % size p
        append 'seq p\[i]
        remove 'p .index i
    ]
    print ["Prisoner killing order:" chop seq]
    print ["Survivor:" last seq]
    print ""
]

print "josephus 5 2 =>"
josephus 5 2

print "josephus 41 3 =>"
josephus 41 3
