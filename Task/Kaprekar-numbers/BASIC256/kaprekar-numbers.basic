n = 0
for i = 1 to 1999999
	if Kaprekar(i) then
		n = n + 1
		if i < 100001 then print n; ": "; i
	endif
next i
print
print "Total de nÃºmeros de Kaprekar por debajo de 1.000.000 = "; n
end

function Kaprekar(n)
	s = n ^ 2
	t = 10 ^ (int(log(s)) + 1)
	do
		t = t / 10
		if t <= n then exit do #break
		if s-n = int(s/t)*(t-1) then return TRUE
	until t <= n
	return n = 1
end function
