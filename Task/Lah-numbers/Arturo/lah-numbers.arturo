factorial: function [n]-> product 1..n

lah: function [n,k][
    if k=1 -> return factorial n
    if k=n -> return 1
    if k>n -> return 0
    if or? k<1 n<1 -> return 0
    return (((factorial n)*factorial n-1) / ((factorial k) * factorial k-1)) / factorial n-k
]

