( ( maxnum
  =   A Z F C
    .   !arg:#
      |   !arg
        :   %@?F
            ?
            ( #%@?C
            & ( str$(!F !C)+-1*str$(!C !F):~<0
              | !C:?F
              )
            & ~
            )
            ?
      | !arg:?A !F ?Z&!F maxnum$(!A !Z)
  )
& out$(str$(maxnum$(1 34 3 98 9 76 45 4)))
& out$(str$(maxnum$(54 546 548 60)))
);
