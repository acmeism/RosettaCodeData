    year = 1908
    select case
        case year mod 400 = 0
            leapYear = 1
        case year mod 4 = 0 and year mod 100 <> 0
            leapYear = 1
        case else
            leapYear = 0
    end select
    if leapYear = 1 then
        print year;" is a leap year."
    else
        print year;" is not a leap year."
    end if
