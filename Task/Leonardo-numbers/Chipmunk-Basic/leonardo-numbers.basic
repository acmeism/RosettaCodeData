100 sub leonardo(l0,l1,suma,texto$)
110  print "Numeros de ";texto$;" (";l0;",";l1;",";suma;"):"
120  for i = 1 to limit
130   if i = 1 then
140   print l0;
150   else
160   if i = 2 then
170      print l1;
180   else
190      print l0+l1+suma;
200      tmp = l0
210      l0 = l1
220      l1 = tmp+l1+suma
230   endif
240   endif
250  next i
260  print chr$(10)
270 end sub
280 limit = 25
290 leonardo(1,1,1,"Leonardo")
300 leonardo(0,1,0,"Fibonacci")
