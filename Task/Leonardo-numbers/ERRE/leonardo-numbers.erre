PROGRAM LEONARDO_NUMBERS

PROCEDURE PRINT_LEONARDO_NUMS(L0,L1,SUM,LMT%,WHAT$)
  ! Print the numbers
  PRINT(WHAT$;" (";L0;",";L1;",";SUM;"):")
  IF LMT%>=1 THEN
    PRINT(L0;)
  END IF
  IF LMT%>=2 THEN
    PRINT(L1;)
  END IF
  FOR I%=3 TO LMT% DO
    PRINT(L0+L1+SUM;)
    TMP=L0
    L0=L1
    L1=TMP+L1+SUM
  END FOR
  PRINT
END PROCEDURE

BEGIN
  PRINT_LEONARDO_NUMS(1,1,1,25,"Leonardo numbers")
  PRINT_LEONARDO_NUMS(0,1,0,25,"Fibonacci numbers")
END PROGRAM
