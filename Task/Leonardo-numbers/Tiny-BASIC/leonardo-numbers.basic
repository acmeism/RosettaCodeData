10 REM Leonardo numbers
20 N=21
30 K=1
40 L=1
50 S=1
60 PRINT "Leonardo numbers (";K;", ";L;", ";S;"):"
70 GOSUB 140
80 K=0
90 L=1
100 S=0
110 PRINT "Fibonacci numbers (";K;", ";L;", ";S;"):"
120 GOSUB 140
130 END
140 IF N<1 GOTO 260
150 PRINT K;" ";
160 IF N<2 GOTO 260
170 PRINT L;" ";
180 I=3
190 IF I>N GOTO 260
200 PRINT K+L+S;" ";
210 T=K
220 K=L
230 L=T+L+S
240 I=I+1
250 GOTO 190
260 PRINT
270 RETURN
