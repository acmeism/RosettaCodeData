      PROClogic(FALSE, FALSE)
      PROClogic(FALSE, TRUE)
      PROClogic(TRUE, FALSE)
      PROClogic(TRUE, TRUE)
      END

      DEF PROClogic(a%, b%)
      LOCAL @% : @% = 2 : REM Column width
      PRINT a% " AND " b% " = " a% AND b% TAB(20);
      PRINT a% " OR "  b% " = " a% OR b%  TAB(40);
      PRINT a% " EOR " b% " = " a% EOR b% TAB(60);
      PRINT " NOT " a% " = " NOT a%
      ENDPROC
