function boolOps p1, p2
    local boolOpsResult
    put p1 && "AND" && p2 && "=" && merge("[[p1 and p2]]") & cr after boolOpsResult
    put p1 && "OR" && p2 && "=" && merge("[[p1 or p2]]") & cr after boolOpsResult
    put "NOT" && p1 && "=" && merge("[[not p1]]")  & cr after boolOpsResult
    return boolOpsResult
end boolOps
