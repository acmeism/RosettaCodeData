repeat for each item bop in "true,false"
  put boolops(bop, bop) & cr after bopResult
  put boolops(bop, not bop) & cr after bopResult
end repeat
put bopResult

-- results
true AND true = true
true OR true = true
NOT true = false

true AND false = false
true OR false = true
NOT true = false

false AND false = false
false OR false = false
NOT false = true

false AND true = false
false OR true = true
NOT false = true
