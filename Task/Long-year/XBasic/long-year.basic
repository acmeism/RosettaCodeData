PROGRAM  "LongYear"
VERSION  "0.0000"

DECLARE FUNCTION Entry ()
DECLARE FUNCTION p (y)
DECLARE FUNCTION isLongYear (y)

FUNCTION  Entry ()
  FOR y = 2000 TO 2100
    IF isLongYear(y) THEN PRINT y,
  NEXT y
END FUNCTION

FUNCTION p (y)
  RETURN (y + INT(y/4) - INT(y/100) + INT(y/400)) MOD 7
END FUNCTION

FUNCTION isLongYear (y)
  RETURN (p(y) = 4) OR (p(y - 1) = 3)
END FUNCTION
END PROGRAM
