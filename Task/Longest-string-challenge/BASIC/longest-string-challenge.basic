DO
   READ test$
   IF test$ = "~" THEN EXIT DO
   IF LEN(test$) > LEN(test1$) THEN
      test1$ = test$
      test2$ = test1$ + CHR$(10)
   ELSEIF LEN(test$) = LEN(test1$) THEN
      LET test2$ = test2$ + test$ + CHR$(10)
   END IF
LOOP

PRINT (test2$)

DATA "a", "bb", "ccc", "ddd", "ee", "f", "ggg", "~" : ' la tilde es solo para mantener el c√≥digo compacto
