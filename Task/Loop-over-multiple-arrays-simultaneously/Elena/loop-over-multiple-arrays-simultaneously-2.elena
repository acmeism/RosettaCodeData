import system'routines.
import extensions.

public program
{
    var a1 := new string[]{"a","b","c"};
    var a2 := new string[]{"A","B","C"};
    var a3 := new int[]{1,2,3};
    var zipped := a1.zipBy(a2,(first,second => first + second.toString() ))
                       .zipBy(a3, (first,second => first + second.toString() ));

    zipped.forEach:(e)
        { console.writeLine:e };

    console.readChar();
}
