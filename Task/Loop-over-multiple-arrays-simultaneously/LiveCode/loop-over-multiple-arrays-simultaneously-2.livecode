command loopDelimitedList
    local lowA, uppA, nums, z
    put "a,b,c" into lowA
    put "A,B,C" into uppA
    put "1,2,3" into nums

    repeat with n = 1 to the number of items of lowA
        put item n of lowA & item n of uppA & item n of nums
& return after z
    end repeat
    put z

end loopDelimitedList
