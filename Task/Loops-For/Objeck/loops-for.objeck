bundle Default {
  class For {
    function : Main(args : String[]) ~ Nil {
      DoFor();
    }
	
    function : native : DoFor() ~ Nil {
    	for (i := 0; i < 5; i += 1;) {
          for (j := 0; j <= i; j += 1;) {
            "*"->Print();
          };
          ""->PrintLine();	
       };
    }
  }
}
