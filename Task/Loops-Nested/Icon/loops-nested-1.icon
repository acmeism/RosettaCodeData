procedure main()

every !(!(L  := list(10)) := list(10))  := ?20   # setup a 2d array of random numbers up to 20

every i := 1 to *L do{                           # using nested loops
   every j := 1 to *L[i] do{
      writes(" ",L[i,j])
      if L[i,j] = 20 then{
         write()
         break break
      }
   }
   write()
}
end
