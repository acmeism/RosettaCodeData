DIM answer$(19)
FOR i = 0 TO UBOUND(answer$): READ answer$(i): NEXT i
RANDOMIZE TIMER

PRINT "Q to quit."
DO
    INPUT "What would you like to know? ", question$
    IF UCASE$(question$) = "Q" THEN EXIT DO
    PRINT answer$(INT(RND * UBOUND(answer$)))
    PRINT
LOOP
END

