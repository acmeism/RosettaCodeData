RANDOMIZE
PRINT "Q to quit."
DO
   INPUT prompt "What would you like to know? ": question$
   IF ucase$(question$) = "Q" then EXIT DO
   PRINT answer$(int(rnd*ubound(answer$)))
   PRINT
LOOP
END
