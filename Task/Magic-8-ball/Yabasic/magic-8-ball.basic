dim answer$(19)
for i = 0 to arraysize(answer$(),1): read answer$(i): next i

print "Q to quit."
do
    input "What would you like to know? " question$
    if upper$(question$) = "Q" then end : fi
    print answer$(int(ran(arraysize(answer$(),1))))
    print
loop

