10 REM MAGIC CONSTANT
20 PRINT "FIRST 20 MAGIC CONSTANTS:"
30 LET N=3
40 IF N>20+3-1 GOTO 80
50 PRINT (N*N*N+N)/2,
60 LET N=N+1
70 GOTO 40
80 PRINT
90 PRINT "SMALLEST ORDER MAGIC SQUARE WITH A CONSTANT GREATER THAN:"
100 LET T=10
110 LET M=3
120 LET X=1
130 LET L=(M*M*M+M)/2
140 LET M=M+1
150 IF L<=T GOTO 130
160 PRINT "10^";X;":",M-1
