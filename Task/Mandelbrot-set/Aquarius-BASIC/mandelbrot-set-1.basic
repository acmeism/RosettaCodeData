10 PRINT CHR$(11);
20 CS=12288+1024
30 FOR Y=0 TO 24
40 FOR X=0 TO 39
50 GOSUB 100:POKE CS,COL
60 CS=CS+1
70 NEXT
80 NEXT
90 END
100 XP=X/39*2.7-2
110 YP=Y/24*2-1
120 IT=0:XX=0:YY=0
130 XTEMP=XX*XX - YY*YY + XP
140 YY=2*XX*YY + YP
150 XX=XTEMP
160 IT=IT+1
170 IF XX*XX+YY*YY<4 AND IT<20 THEN 130
180 COL=IT AND 15:IF IT=20 THEN COL=0
190 RETURN
