for y0 = 0 to 299
  cy = (y0 - 150) / 120
  for x0 = 0 to 299
    cx = (x0 - 220) / 120
    x = 0 ; y = 0 ; iter = 0
    while iter < 64 and x * x + y * y < 4
      h = x * x - y * y + cx
      y = 2 * x * y + cy
      x = h
      iter += 1
    .
    if iter = 64
      iter = 0
    .
    color3 iter / 16 0 0
    move x0 / 3 y0 / 3
    rect 0.4 0.4
  .
.
