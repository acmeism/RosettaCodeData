printMatrix: function [m][
    loop m 'row -> print map row 'val [pad to :string .format:".2f" val 6]
    print "--------------------------------"
]

multiply: function [a,b][
    X: size a
    Y: size first b
    result: array.of: @[X Y] 0

    loop 0..X-1 'i [
        loop 0..Y-1 'j [
            loop 0..(size first a)-1 'k ->
                result\[i]\[j]: result\[i]\[j] + a\[i]\[k] * b\[k]\[j]
        ]
    ]
    return result
]

