360 :   N=I*J:GOSUB 520
370 :   IF LEN(N$)<3 THEN PRINT ":";
380 :   PRINT N$;
390 : NEXT J
400 : FOR J=11 TO 12
410 :   W=3
420 :   IF J<I THEN N$=" ":GOSUB 530:GOTO 440
