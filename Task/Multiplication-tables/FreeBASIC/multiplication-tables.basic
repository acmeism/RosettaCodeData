' FB 1.05.0 Win64

Print "  X|";
For i As Integer = 1 To 12
  Print Using "####"; i;
Next

Print
Print "---+"; String(48, "-")

For i As Integer = 1 To 12
  Print Using "###"; i;
  Print"|"; Spc(4 * (i - 1));
  For j As Integer = i To 12
    Print Using "####"; i * j;
  Next j
  Print
Next i

Print
Print "Press any key to quit"
Sleep
