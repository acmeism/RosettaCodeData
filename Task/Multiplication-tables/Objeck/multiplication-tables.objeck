class MulTbl {
	function : Main(args : String[]) ~ Nil {
      for(i := 1; i <= 12; i++;) {
        "\t{$i}"->Print();
      };

	  ""->PrintLine();
	  for(i := 0; i < 100; i++;) {
	    ("-")->Print();
	  };

	  ""->PrintLine();
	  for(i := 1; i <= 12; i++;) {
	    "{$i}|"->Print();
	    for(j := 1; j <= 12; j++) {
	    "\t"->Print();
	    if (j >= i) {
	    	s := i * j;
	      "{$s}"->Print();
	    };
	    };
	    ""->PrintLine();
	  };
	}
}
