for i = 0 to 5

	for j = 0 to 5

		for k = 0 to 5

			for l = 0 to 5

				let m = int(i ^ i * sgn(i))
				let m = m + int(j ^ j * sgn(j))
				let m = m + int(k ^ k * sgn(k))
				let m = m + int(l ^ l * sgn(l))

				let n = 1000 * i + 100 * j + 10 * k + l

				if m = n and m > 0 then

					print m

				endif

				wait

			next l

		next k

	next j

next i
