Module M端nchhausen {
      Dim p(0 to 9)
      p(0)=0, 1
      for i=2 to 9 {p(i)=i**i}
      M端nchhausen=lambda p() (x)-> {
            m=0
            t=x
            do {
                  m+=p(x mod 10)
                  x=x div 10
            } until x=0
            =m=t
      }
      For i=1 to 5000
            If M端nchhausen(i) then print i,
      Next i
      Print
}
M端nchhausen
