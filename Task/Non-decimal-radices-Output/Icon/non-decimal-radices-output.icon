procedure main()
write("Non-decimal radices/Output")
every i := 255 | 2 | 5 | 16 do {
   printf("%%d = %d\n",i) # integer format
   printf("%%x = %x\n",i) # hex format
   printf("%%o = %o\n",i) # octal format
   printf("%%s = %s\n",i) # string format
   printf("%%i = %i\n",i) # image format
   }
end
