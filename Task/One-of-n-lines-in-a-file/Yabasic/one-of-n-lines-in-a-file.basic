dim elegido(10)

sub one_of_n (n)
    //asume que la primera l√≠nea es 1
    local L1
    for L1 = 1 to n
        if int(ran(L1)) = 0 then opcion = L1 : endif
    next L1
    return opcion
end sub

for L0 = 1 to 1000000
    c = one_of_n(10)
    elegido(c) = elegido(c) + 1
next L0

for L0 = 1 to 10
    print L0, ".  ", elegido(L0)
next L0

end
