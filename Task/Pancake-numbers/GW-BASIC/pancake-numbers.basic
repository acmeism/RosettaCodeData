100 CLS
110 FOR I = 0 TO 3
120  FOR J = 1 TO 5
130   N = (I*5)+J
140   C = C+1
150   GOSUB 200
160   PRINT USING "p(##) = ##  ";N;PANCAKE
170  NEXT J
180 NEXT I
190 END
200 REM pancake(n)
210  GAP = 2
220  SUM = 2
230  ADJ = -1
240  IF SUM < N THEN ADJ = ADJ+1 : GAP = (GAP*2)-1 : SUM = SUM+GAP
250  IF SUM >= N THEN PANCAKE = N+ADJ : RETURN
260 GOTO 240
