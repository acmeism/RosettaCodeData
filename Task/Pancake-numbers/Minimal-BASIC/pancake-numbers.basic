110 FOR I = 0 TO 3
120  FOR J = 1 TO 5
130   LET N = (I*5)+J
150   GOSUB 200
160   PRINT "P(";N;") =";P;" "
170  NEXT J
180 NEXT I
190 GOTO 330
200 REM PANCAKE(N)
210  LET G = 2
220  LET S = 2
230  LET A = -1
240  IF S < N THEN 270
250  IF S >= N THEN 310
260 GOTO 240
270  LET A = A+1
280  LET G = (G*2)-1
290  LET S = S+G
300 GOTO 250
310  LET P = N+A
320 RETURN
330 END
