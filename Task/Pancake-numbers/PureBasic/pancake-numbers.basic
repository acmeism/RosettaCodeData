Procedure pancake(n)
  gap.i = 2
  sum.i = 2
  adj.i = -1
  While sum < n
    adj + 1
    gap = (gap * 2) - 1
    sum + gap
  Wend
  ProcedureReturn n + adj
EndProcedure

OpenConsole()
Define.i i, j, c, n

For i = 0 To 3
  For j = 1 To 5
    n = (i * 5) + j
    c + 1
    Print("p(" + RSet(Str(n),2) + ") = " + RSet(Str(pancake(n)),2) + "  ")
    If Mod(c, 5 )= 0: PrintN(""): EndIf
  Next j
Next i

Input()
CloseConsole()
