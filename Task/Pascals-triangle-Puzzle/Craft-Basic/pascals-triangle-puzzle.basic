let x = -1

do

	let x = x + 1
	let z = 0

	do

		let e = x + 11
		let f = 11 + (x + z)
		let g = (x + z) + 4
		let h = 4 + z

		if e + f = 40 then

			let c = f + g
			let d = g + h
			let a = 40 + c
			let b = c + d
			let q = 0

			if a + b = 151 then

				let q = 1

			endif

		endif

		if q = 0 then

			let z = z + 1

		endif

		wait

	loopwhile z < 20 and q = 0

	if q = 0 then

		let z = -1

	endif

	wait

loopuntil z >= 0

print "x = ", x
print "y = ", x + z
print "z = ", z
