# Known:
#         [ 151]
#        [a ][b ]
#      [40][c ][d ]
#    [e ][f ][g ][h ]
#  [ X][11][ Y][ 4][ Z]
#
#  Y = X + Z
#
repeat
   x += 1
   e = x + 11
   f = 40 - e
   y = f - 11
   g = y + 4
   c = f + g
   a = c + 40
   b = 151 - a
   d = b - c
   h = d - g
   z = h - 4
   until y = x + z
.
numfmt 3 0
print "       " & 151
print "     " & a & b
print "   " & 40 & c & d
print "  " & e & f & g & h
print x & 11 & y & 4 & z
