bundle Default {
  class Test {
    function : Main(args : String[]) ~ Nil {
      "Perfect numbers from 1 to 33550337:"->PrintLine();
      for(num := 1 ; num < 33550337; num += 1;) {
        if(IsPerfect(num)) {
          num->PrintLine();
        };
      };
    }
		
    function : native : IsPerfect(number : Int) ~ Bool {
      sum := 0 ;
      for(i := 1; i < number; i += 1;) {
        if (number % i = 0) {
          sum += i;
        };
      };

      return sum = number;
    }
  }
}
