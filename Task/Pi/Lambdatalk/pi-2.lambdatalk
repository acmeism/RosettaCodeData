{script
LAMBDATALK.DICT["spigot"] = function() {
  function generateDigitsOfPi(max) {
    var pi = "";
    var z = 0;
    var q = 1n;
    var r = 180n;
    var t = 60n;
    var i = 2n;
    while (z < max) {
        var digit = ((i * 27n - 12n) * q + r * 5n) / (t * 5n);
        pi += digit;
        var  u = (i * 3n + 1n) * 3n * (i * 3n + 2n);
        r = u * 10n * (q * (i * 5n - 2n) + (r - t * digit));
        q = q * 10n * i * (i * 2n - 1n);
        i = i + 1n;
        t = t * u;
        z++;
    }
    return pi
  }
  var args = arguments[0].trim();
  return generateDigitsOfPi( args );
};
}

