100 CLS
110 PRINT "Primes below 100": PRINT
120 FOR n = 2 TO 100
130  GOSUB 160
140 NEXT n
150 GOTO 250
160 rem FUNCTION WilsonPrime(n)
170  LET f = 1
180  FOR i = 2 TO n-1
181   LET a = f * i
190   LET f = a - INT(a / n) * n
200  NEXT i
210  IF f = n-1 THEN 230
220 RETURN
230 PRINT i;"  ";
240 RETURN
250 END
