scope # find some primes by trial division

    local constant is_prime := proc( n :: number ) :: boolean
        local result := n = 2 or ( odd n and n > 1 );
        for k from 3 to entier sqrt( n ) by 2 while result do
            result := n mod k <> 0
        od;
        return result
    end;

    for p from 0 to 100 do if is_prime( p ) then printf( " %d", p ) fi od

end
