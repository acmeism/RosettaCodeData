100 PROGRAM "Prime.bas"
110 FOR X=0 TO 100
120   IF PRIME(X) THEN PRINT X;
130 NEXT
140 DEF PRIME(N)
150   IF N=2 THEN
160     LET PRIME=-1
170   ELSE IF N<=1 OR MOD(N,2)=0 THEN
180     LET PRIME=0
190   ELSE
200     LET PRIME=-1
210     FOR I=3 TO CEIL(SQR(N)) STEP 2
220       IF MOD(N,I)=0 THEN LET PRIME=0:EXIT FOR
230     NEXT
240   END IF
250 END DEF
