conspiracy(maxx)={
print("primes considered= ",maxx);
x=matrix(9,9);cnt=0;p=2;q=2%10;
while(cnt<=maxx,
cnt+=1;
m=q;
p=nextprime(p+1);
q= p%10;
x[m,q]+=1);
print (2," to ",3, " count: ",x[2,3],"  freq ", 100./cnt,"  %" );
forstep(i=1,9,2,
forstep(j=1,9,2,
if( x[i,j]<1,continue);
print (i," to ",j, " count: ",x[i,j],"  freq ", 100.* x[i,j]/cnt,"  %" )));
print ("total transitions= ",cnt);
print(p);
}
