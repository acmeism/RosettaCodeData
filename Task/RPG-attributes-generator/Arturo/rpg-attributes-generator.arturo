vals: []

while [or? 75 > sum vals
            2 > size select vals => [&>=15]] [
    vals: new []

    while [6 > size vals][
        rands: new map 1..4 => [random 1 6]
        remove 'rands .once (min rands)
        'vals ++ sum rands
    ]
]

print ["values:" vals ]
print ["with sum:" sum vals]
