import kotlin.math.*

fun main() {
    println(E)
    println(PI)
    println(sqrt(2.0))
    println(ln(E))
    println(log10(10.0))
    println(log(4.0, 2.0)) // log base 2 of 4
    println(exp(1.0))
    println(abs(-1))
    println(floor(-2.5))
    println(ceil(-2.5))
    println(2.5.pow(3.5)) // 2.5^3.5
}
