a$ =" 1 $23.19 2 elbow 3 2 Bork 4 3 elbow 2 $23.19 "
print "Original set of elements = ["; a$; "]"

b$ =removeDuplicates$( a$)
print "With duplicates removed  = ["; b$; "]"

end

function removeDuplicates$( in$)
    o$ =" "
    i  =1
    do
        term$    =word$( in$, i, " ")
        if instr( o$, " "; term$; " ") =0 and term$ <>" " then o$ =o$ +term$ +" "
        i        =i +1
    loop until term$ =""
    removeDuplicates$ =o$
end function
