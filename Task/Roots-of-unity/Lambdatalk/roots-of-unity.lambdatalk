// cleandisp just to display 0 when n < 10^-10
{def cleandisp
 {lambda {:n}
  {if {<= {abs :n} 1.e-10} then 0 else :n}}}
-> cleandisp

{def uroots
 {lambda {:n}
  {S.map {{lambda {:n :i}
                  {let { {:theta {/ {* 2 {PI} :i} :n}}
                       } {cons {cleandisp {cos :theta}}
                               {cleandisp {sin :theta}}}}} :n}
         {S.serie 0 {- :n 1}}} }}
-> uroots

