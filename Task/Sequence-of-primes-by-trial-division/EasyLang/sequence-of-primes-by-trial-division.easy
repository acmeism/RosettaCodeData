proc isPrime num . result .
   result = 0
   if num < 2
      break 1
   .
   if num mod 2 = 0 and num > 2
      break 1
   .
   for i = 3 step 2 to sqrt num
      if num mod i = 0
         break 2
      .
   .
   result = 1
.
proc primeSequence first last . sequence[] .
   for i = first to last
      call isPrime i result
      if result = 1
         sequence[] &= i
      .
   .
.
call primeSequence 1 100 seq[]
print seq[]
