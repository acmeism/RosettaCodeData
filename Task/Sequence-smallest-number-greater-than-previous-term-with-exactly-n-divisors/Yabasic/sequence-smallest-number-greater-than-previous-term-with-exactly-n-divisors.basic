UPTO = 15
i = 2
nfound = 1

print 1, " ";    //special case

while nfound < UPTO
    n = divisors(i)
    if n = nfound + 1 then
        nfound = nfound + 1
        print i, " ";
    fi
    i = i + 1
end while
print
end

sub divisors(n)
    local r, i

    //find the number of divisors of an integer
    r = 2
    for i = 2 to n / 2
        if mod(n, i) = 0  r = r + 1
    next i
    return r
end sub
