' Short-circuit evaluation
FUNCTION a(f)
    PRINT "FUNCTION a"
    RETURN f
END FUNCTION

FUNCTION b(f)
    PRINT "FUNCTION b"
    RETURN f
END FUNCTION

PRINT "FALSE and TRUE"
x = a(FALSE) AND b(TRUE)
PRINT x

PRINT "TRUE and TRUE"
x = a(TRUE) AND b(TRUE)
PRINT x

PRINT "FALSE or FALSE"
y = a(FALSE) OR b(FALSE)
PRINT y

PRINT "TRUE or FALSE"
y = a(TRUE) OR b(FALSE)
PRINT y
