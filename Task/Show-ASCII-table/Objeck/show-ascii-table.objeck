class AsciiTable {
  function : Main(args : String[]) ~ Nil {
    for(i := 32; i <= 127 ; i += 1;) {
      if(i = 32 | i = 127) {
        s := i = 32 ? "Spc" : "Del";
        "{$i}:\t{$s}\t"->Print();
      }
      else {
        c := i->ToChar();
        "{$i}:\t{$c}\t"->Print();
      };
      if((i-1) % 6 = 0 ) {
        "\n"->Print();
      };
    };
  }
}
