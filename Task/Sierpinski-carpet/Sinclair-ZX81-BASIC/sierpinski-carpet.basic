 10 LET O=3
 20 LET S=3**O
 30 FOR I=0 TO S-1
 40 FOR J=0 TO S-1
 50 LET X=J
 60 LET Y=I
 70 GOSUB 120
 80 IF C THEN PLOT J,I
 90 NEXT J
100 NEXT I
110 GOTO 190
120 LET C=0
130 IF X-INT (X/3)*3=1 AND Y-INT (Y/3)*3=1 THEN RETURN
140 LET X=INT (X/3)
150 LET Y=INT (Y/3)
160 IF X>0 OR Y>0 THEN GOTO 130
170 LET C=1
180 RETURN
