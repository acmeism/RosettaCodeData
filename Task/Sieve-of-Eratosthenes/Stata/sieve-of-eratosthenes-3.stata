mata
real colvector sieve(real scalar n) {
	real colvector a
	real scalar i, j
	if (n < 2) return(J(0, 1, .))
	a = J(n, 1, 1)
	a[1] = 0
	for (i = 1; i <= n; i++) {
		if (a[i]) {
			j = i*i
			if (j > n) return(select(1::n, a))
			for (; j <= n; j = j+i) a[j] = 0
		}
	}
}

sieve(10)
       1
    +-----+
  1 |  2  |
  2 |  3  |
  3 |  5  |
  4 |  7  |
    +-----+
end
