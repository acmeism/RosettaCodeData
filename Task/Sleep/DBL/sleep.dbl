;
;       Sleep for DBL version 4 by Dario B.
;
        PROC
;------------------------------------------------------------------
        XCALL FLAGS (0007000000,1)          ;Suppress STOP message

        OPEN(1,O,'TT:')
        DISPLAY (1,"Sleeping...",10)
        SLEEP 10                            ;Sleep for 10 seconds
        DISPLAY (1,"Awake!",10)
