import sort

// sorting string literals
x = "lion, tigers, and"
y = "bears, oh my!"
z = "(from the \"Wizard of OZ\")"

varlist = sort({x,y,z})

x = varlist[0]
y = varlist[1]
z = varlist[2]
println x; println y; println z

// sorting integers
x = 77444
y = -12
z = 0

varlist = sort({x, y, z})

x = varlist[0]
y = varlist[1]
z = varlist[2]
println x; println y; println z
