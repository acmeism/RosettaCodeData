function : GnomeSort(a : Int[]) {
  i := 1;
  j := 2;
Â 
  while(i < a->Size()) {
    if (a[i-1] <= a[i]) {
      i := j;
      j += 1;
    }
    else {
      tmp := a[i-1];
      a[i-1] := a[i];
      a[i] := tmp;
      i -= 1;
      if(i = 0) {
        i := j;
        j += 1;
      };
    };
  };
}
