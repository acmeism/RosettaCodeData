insertionSort: function [items][
    arr: new items
    loop 1..(size items)-1 'i [
        value: arr\[i]
        j: i - 1

        while [and? -> j >= 0
                    -> value < arr\[j]]
        [
            arr\[j+1]: arr\[j]
            j: j - 1
        ]
        arr\[j+1]: value
    ]
    return arr
]

print insertionSort [3 1 2 8 5 7 9 4 6]
