!YS-v0

defn main(input):
  each line input:lines:
    say: line:words.map(N):sparkline

defn sparkline(nums):
  sparks =: '▁▂▃▄▅▆▇█'
  high =: max(nums*)
  low =: min(nums*)
  spread =: high - low
  quantize =:
    \(round(sparks.#.-- * ((_ - low) / spread)))
  apply str: nums.map(\(nth sparks quantize(_)))
