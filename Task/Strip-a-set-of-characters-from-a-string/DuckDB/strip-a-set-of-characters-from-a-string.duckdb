# Omit from `str` each of the characters in `omit` :
create or replace function string_strip(str, omit) as (
  array_to_string(
    regexp_split_to_array(
      str,
      '[' || regexp_escape( omit ) || ']'), '')
);

# Example:
select string_strip('She was a soul stripper. She took my heart!', 'aei') as translated;
