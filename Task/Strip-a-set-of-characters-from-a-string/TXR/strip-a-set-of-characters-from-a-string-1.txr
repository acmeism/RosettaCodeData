@(do
   (defun strip-chars (str set)
     (let* ((regex-ast ^(set ,*(list-str set)))
            (regex-obj (regex-compile regex-ast)))
       (regsub regex-obj "" str)))

   (defun usage ()
     (pprinl `usage: @{(ldiff *full-args* *args*) " "} <string> <set>`)
     (exit 1))

   (tree-case *args*
     ((str set extra) (usage))
     ((str set . junk) (pprinl (strip-chars str set)))
     (else (usage))))
