      REM Remove leading whitespace:
      WHILE ASC(A$)<=32 A$ = MID$(A$,2) : ENDWHILE

      REM Remove trailing whitespace:
      WHILE ASC(RIGHT$(A$))<=32 A$ = LEFT$(A$) : ENDWHILE

      REM Remove both leading and trailing whitespace:
      WHILE ASC(A$)<=32 A$ = MID$(A$,2) : ENDWHILE
      WHILE ASC(RIGHT$(A$))<=32 A$ = LEFT$(A$) : ENDWHILE
