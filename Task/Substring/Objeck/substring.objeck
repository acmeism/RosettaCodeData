bundle Default {
  class SubString {
    function : Main(args : String[]) ~ Nil {
      s := "0123456789";

      n := 3;
      m := 4;
      c := '2';
      sub := "456";

      s->SubString(n, m)->PrintLine();
      s->SubString(n)->PrintLine();
      s->SubString(0, s->Size())->PrintLine();
      s->SubString(s->Find(c), m)->PrintLine();
      s->SubString(s->Find(sub), m)->PrintLine();
    }
  }
}
