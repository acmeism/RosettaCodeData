func KtoC(k : float) -> float { k - 273.15 }
func KtoF(k : float) -> float { k * 1.8 - 459.67 }
func KtoR(k : float) -> float { k * 1.8 }

func convertK(k : float) -> int {
    prints("K " + k + "\n");
    prints("C " + KtoC(k) + "\n");
    prints("F " + KtoF(k) + "\n");
    prints("R " + KtoR(k) + "\n");
    0
}

func main(k : float) -> int {
    convertK(k);
    0
}
