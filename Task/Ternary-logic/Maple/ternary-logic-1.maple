tv := [true, false, FAIL];
NotTable     := Array(1..3, i->not tv[i] );
AndTable     := Array(1..3, 1..3, (i,j)->tv[i] and tv[j] );
OrTable      := Array(1..3, 1..3, (i,j)->tv[i] or tv[j] );
XorTable     := Array(1..3, 1..3, (i,j)->tv[i] xor tv[j] );
ImpliesTable := Array(1..3, 1..3, (i,j)->tv[i] implies tv[j] );
