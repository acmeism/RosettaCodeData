(   0:?N:?n
  & :?Ts
  & @( get$("mlijobs.txt",STR)
     :   ?
         ( "e " ?OI " @ " ?T " " ?
         &   (   !OI:OUT
               &   !n+1
                 : ( >!N:?N&!T:?Ts
                   | !N&!Ts !T:?Ts
                   | ?
                   )
             | !n+-1
             )
           : ?n
         & ~
         )
     )
| out$(!N !Ts)
);
