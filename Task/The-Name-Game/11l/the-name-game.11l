F print_verse(n)
   V l = [String(‘b’), ‘f’, ‘m’]
   V s = n[1..]
   V? i = l.find(n[0].lowercase())
   I i != N
      l[i] = ‘’
   E I n[0] C (‘A’, ‘E’, ‘I’, ‘O’, ‘U’)
      s = n.lowercase()
   print("#., #., bo-#.#.\nBanana-fana fo-#.#.\nFee-fi-mo-#.#.\n#.!\n".format(n, n, l[0], s, l[1], s, l[2], s, n))

L(n) [‘Gary’, ‘Earl’, ‘Billy’, ‘Felix’, ‘Mary’]
   print_verse(n)
