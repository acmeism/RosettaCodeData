create or replace function TM(n) as (
create or replace function TM(n) as (
  with recursive mx as (select (ceil(log(n) / log(2))) as mx),
  cte as (
    select '0' as sb0, '1' as sb1, 0 as ix
    union all
    select (sb0 || sb1) as sb0,
           (sb1 || sb0) as sb1,
           ix+1,
           from cte, mx
           where ix < mx
  )
  select last(sb0[:n] order by ix) from cte
);
## Example:
select TM(100);
