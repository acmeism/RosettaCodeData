dim s(11)
print 'enter 11 numbers'
for i = 0 to 10
   input i + ">" , s[i]
next i

for i = 10 to 0 step -1
   print "f(" + s[i] + ")=";
   x = f(s[i])
   if x > 400 then
      print "--- too large ---"
   else
      print x
   endif
next i
end

function f(n)
   return sqrt(abs(n))+5*n^3
end function
