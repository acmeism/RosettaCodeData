-- parent script "BinaryTreeNode"

property _val, _left, _right

on new (me, val)
  me._val = val
  return me
end

on getValue (me)
  return me._val
end

on setLeft (me, node)
  me._left = node
end

on setRight (me, node)
  me._right = node
end

on getLeft (me)
  return me._left
end

on getRight (me)
  return me._right
end
