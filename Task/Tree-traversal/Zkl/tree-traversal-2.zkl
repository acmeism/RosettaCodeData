t:=BTree(Node(1,
	   Node(2,
	      Node(4,Node(7)),
	      Node(5)),
	   Node(3,
	      Node(6, Node(8),Node(9)))));

t.preOrder()  .apply("v").println("  preorder");
t.inOrder()   .apply("v").println("  inorder");
t.postOrder() .apply("v").println("  postorder");
t.levelOrder().apply("v").println("  level-order");
