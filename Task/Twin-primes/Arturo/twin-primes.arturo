pairsOfPrimes: function [upperLim][
    count: 0
    j: 0
    k: 1
    i: 0
    while [i=<upperLim][
        i: (6 * k) - 1
        j: i + 2
        if and? [prime? i] [prime? j] [
            count: count + 1
        ]
        k: k + 1
    ]
    return count + 1
]

ToNum: 10
while [ToNum =< 1000000][
    x: pairsOfPrimes ToNum
    print ["From 2 to" ToNum ": there are" x "pairs of twin primes"]
    ToNum: ToNum * 10
]
