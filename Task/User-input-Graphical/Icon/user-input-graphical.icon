procedure main()
WOpen("size=800,800") | stop("Unable to open window")
WWrite("Enter a string:")
s := WRead()
WWrite("You entered ",image(s))
WWrite("Enter the integer 75000:")
i := WRead()
if i := integer(i) then
   WWrite("You entered: ",i)
else
   WWrite(image(i)," isn't an integer")
WDone()
end

