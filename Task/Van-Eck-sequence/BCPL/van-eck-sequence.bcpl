get "libhdr"

let start() be
$(  let eck = vec 999

    for i = 0 to 999 do eck!i := 0
    for i = 0 to 998 do
        for j = i-1 to 0 by -1 do
            if eck!i = eck!j then
            $(  eck!(i+1) := i-j
                break
            $)

    for i = 0 to 9 do writed(eck!i, 4)
    wrch('*N')
    for i = 990 to 999 do writed(eck!i, 4)
    wrch('*N')
$)
