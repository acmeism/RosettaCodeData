module VariadicFunction {
    void show(String[] strings) {
        @Inject Console console;
        strings.forEach(s -> console.print(s));
    }

    void run() {
        show(["hello", "world"]);

        String s1 = "not";
        String s2 = "a";
        String s3 = "constant";
        String s4 = "literal";
        show([s1, s2, s3, s4]);
    }
}
