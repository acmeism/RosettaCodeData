a = [3, 4, 5];
b = [4, 3, 5];
c = [-5, -12, -13];

function r = s3prod(a, b, c)
  r = dot(a, cross(b, c));
endfunction

function r = v3prod(a, b, c)
  r = cross(a, cross(b, c));
endfunction

% 49
dot(a, b)
