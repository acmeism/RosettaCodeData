dot(u,v)={
  sum(i=1,#u,u[i]*v[i])
};
cross(u,v)={
  [u[2]*v[3] - u[3]*v[2], u[3]*v[1] - u[1]*v[3], u[1]*v[2] - u[2]*v[1]]
};
striple(a,b,c)={
  dot(a,cross(b,c))
};
vtriple(a,b,c)={
  cross(a,cross(b,c))
};

a = [3,4,5]; b = [4,3,5]; c = [-5,-12,-13];
dot(a,b)
cross(a,b)
striple(a,b,c)
vtriple(a,b,c)
