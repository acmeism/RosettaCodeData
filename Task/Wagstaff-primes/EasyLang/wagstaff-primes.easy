proc prime v . r .
   r = 1
   if v mod 2 = 0 or v mod 3 = 0
      if v <> 2 and v <> 3
         r = 0
      .
      break 1
   .
   d = 5
   while d * d <= v
      if v mod d = 0
         r = 0
         break 2
      .
      d += 2
   .
.
pri = 1
nwag = 0
while nwag <> 10
   pri += 2
   call prime pri r
   if r = 1
      wag = (pow 2 pri + 1) / 3
      call prime wag r
      if r = 1
         nwag += 1
         print pri & " => " & wag
      .
   .
.
