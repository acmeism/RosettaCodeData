Screen 19
Color ,7
Cls

Sub Taijitu(x As Integer, y As Integer, r As Integer)
    Circle(x, y), 2 * r, 0,,,, F
    Line (x, y - 2 * r) - (x, y + 2 * r), 7, B
    Paint (x - r, y), 15, 7
    Circle(x, y - r), r - 1, 15,,,, F
    Circle(x, y + r), r - 1,  0,,,, F
    Circle(x, y - r), r / 3,  0,,,, F
    Circle(x, y + r), r / 3, 15,,,, F
End Sub

Taijitu(110, 110, 45)
Taijitu(500, 300, 138)
End
