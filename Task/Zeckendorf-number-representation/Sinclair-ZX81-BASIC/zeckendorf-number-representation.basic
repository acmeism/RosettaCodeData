 10 DIM F(6)
 20 LET F(1)=1
 30 LET F(2)=2
 40 FOR I=3 TO 6
 50 LET F(I)=F(I-2)+F(I-1)
 60 NEXT I
 70 FOR I=0 TO 20
 80 LET Z$=""
 90 LET S$=" "
100 LET Z=I
110 FOR J=6 TO 1 STEP -1
120 IF J=1 THEN LET S$="0"
130 IF Z<F(J) THEN GOTO 180
140 LET Z$=Z$+"1"
150 LET Z=Z-F(J)
160 LET S$="0"
170 GOTO 190
180 LET Z$=Z$+S$
190 NEXT J
200 PRINT I;" ";
210 IF I<10 THEN PRINT " ";
220 PRINT Z$
230 NEXT I
